[user]
[alias]
  s = status -s --branch
  c = commit -m
  cm = commit
  ca = commit -a
  cp = !git commit && git push
  cap = !git commit -a && git push
  co = checkout
  cb = checkout -b
  pop = reset HEAD~1
  f = fetch
  p = push
  aa = add -A
  log = log --oneline --decorate --graph --all
  l = log --oneline --decorate --graph --all
  branch = branch -vv
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
[user]
	name = Leonard Schuetz
	email = leni.schuetz@me.com
[push]
	default = simple
[color]
	ui = auto
[commit]
	gpgsign = true
[github]
	user = KCreate
[ghi]
	token = !security find-internet-password -a KCreate -s github.com -l 'ghi token' -w
[core]
	editor = '/usr/local/bin/gvim' -f -i NONE
[gpg]
	program = /usr/local/bin/gpg
